{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/datas/*": [
        "datas/*"
      ],
      "@/interfaces/*": [
        "interfaces/*"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/assets/*": [
        "assets/*"
      ],
      "@/store/*": [
        "store/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/config/*": [
        "config/*"
      ],
      "@/lib/*": [
        "lib/*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "config/index.js",
    "pages/mdx-remote.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
